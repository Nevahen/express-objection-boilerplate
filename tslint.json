{
  "defaultSeverity": "error",
  "extends": [
      "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [true, "single"],
    "object-literal-sort-keys": false,
    "member-ordering": false,
    "variable-name": false,
    "arrow-parens": [true, "ban-single-arg-parens"],
    "member-access": false,
    "no-bitwise": false,
    "no-console": false,
    "no-var-requires": false
  },
  "rulesDirectory": []
}